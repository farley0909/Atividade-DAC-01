generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id          Int      @id @default(autoincrement())
  title       String
  releaseDate DateTime
}

model Publisher {
  code        Int    @id @default(autoincrement())
  originPlace String
  fantasyName String
}
