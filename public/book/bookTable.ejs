<table>
  <thead class="font-medium">
    <td class="p-2">Título</td>
    <td>Data de lançamento</td>
  </thead>
  <% books.map(book =>{ %>
  <tr class="hover:bg-white/10" tabindex="1">
    <td class="p-2"><%= book.title%></td>
    <td class="opacity-50">
      <%= new Date(book.releaseDate).toLocaleDateString() %>
    </td>
    <td>
      <div class="flex-row w-fit">
        <button class="hover:bg-blue-500 p-2 px-4 md:px-6 rounded-lg">
          <span class="material-symbols-outlined">edit</span>
        </button>
        <button class="hover:bg-red-500 p-2 px-4 md:px-6 rounded-lg">
          <span class="material-symbols-outlined">delete</span>
        </button>
      </div>
    </td>
  </tr>
  <% }) %>
</table>
