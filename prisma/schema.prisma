generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Livro {
  id               Int      @id @default(autoincrement())
  titulo           String
  dataDeLancamento DateTime
}

model Editora {
  codigo        Int    @id @default(autoincrement())
  localDeOrigem String
  nomeFantasia  String
}
